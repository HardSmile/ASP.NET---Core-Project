@model BecomeDealerFormModel
@{
    ViewBag.Title = "Become Dealer";
}
<div class="container">
    <div class="row">
        <div class="offset-md-2 col-md-8 col-sm-12">
            <form method="post" enctype="multipart/form-data">
                <h2>@ViewBag.Title</h2>
                <div class="form-group">
                    <label asp-for="Name"></label>
                    <input asp-for="Name" class="form-control" placeholder="Best cars">
                    <span asp-validation-for="Name" class="small text-danger"></span>
                </div>  <div class="form-group">
                    <label asp-for="PhoneNumber"></label>
                    <input asp-for="PhoneNumber" class="form-control" placeholder="Phone Number">
                    <span asp-validation-for="PhoneNumber" class="small text-danger"></span>
                </div>  
               
                <input class="btn btn-primary" type="submit" value="Save" />
            </form>
        </div>
    </div>
</div>